{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie >= 9"
          ]
        }
      }
    ],
    "@babel/preset-react",
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": "always"
      }
    ]
  ],
  "plugins": [
    "@emotion",
    "@babel/plugin-proposal-class-properties"
  ]
}